{
  "source":      "./src",
  "destination": "./docs",
  "plugins": [{
    "name": "esdoc-flow-plugin"
  }],
  "experimentalProposal": {
    "classProperties": true,
    "objectRestSpread": true,
    "decorators": true,
    "doExpressions": true,
    "functionBind": true,
    "asyncGenerators": true,
    "exportExtensions": true,
    "dynamicImport": true
  }
}
